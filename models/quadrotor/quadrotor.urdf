<?xml version="1.0"?>
<robot name="physics">

<link name="world">
    <inertial>
        <mass value="0"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
</link>
<joint name="floating_base" type="fixed">
    <parent link="world"/>
    <child link="quadrotor"/>
</joint>
<link name="quadrotor">
    <visual>
        <geometry>
            <mesh filename="package://quadrotor.obj" scale=".1 .1 .1"/>/>
        </geometry>
    </visual>
    <inertial>
        <mass value="1"/>
        <inertia ixx="0.0046" ixy="0.0" ixz="0.0" iyy="0.0046" iyz="0.0" izz="0.008"/>
    </inertial>
    <collision>
        <geometry>
            <box size="0.05 0.05 1.0"/>	
        </geometry>
        <origin rpy="0 0 0" xyz="0 0 0.5"/>
    </collision>
</link>
  
</robot>
